{
  "cli": {
    "version": ">= 10.2.4"
  },
  "build": {
    "development": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "developmentClient": true
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "APP_ID": "com.trajano.myapp",
        "APP_NAME": "My App"
      }
    },
    "production": {
      "extends": "preview",
      "distribution": "store",
      "android": {
        "buildType": "app-bundle"
      }
    },
    "development-simulator": {
      "extends": "development",
      "ios": {
        "simulator": true
      }
    }
  }
}
