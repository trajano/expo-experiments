{
  "name": "react-native-my-components",
  "description": "Styled components",
  "main": "./dist/index.js",
  "types": "./src/index.ts",
  "react-native": "./src/index.ts",
  "scripts": {
    "dev": "tsup --watch",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "prepare": "tsup"
  },
  "devDependencies": {
    "@shopify/flash-list": "^1.7.1",
    "@testing-library/react-native": "^12.7.2",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.2.44",
    "babel-jest": "^29.7.0",
    "eslint-config-my": "*",
    "expo-file-system": "^17.0.1",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-file-viewer": "^2.1.5",
    "react-native-my-text": "*",
    "react-test-renderer": "18.2.0",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "@shopify/flash-list": "*",
    "expo-file-system": "*",
    "react-native-file-viewer": "*",
    "react": "*",
    "react-native": "*"
  },
  "jest": {
    "preset": "jest-expo",
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
