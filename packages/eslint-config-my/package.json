{
  "name": "eslint-config-my",
  "description": "My ESLint config",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsup --watch",
    "test": "jest --passWithNoTests",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "prepare": "tsup"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@types/jest": "^29.5.5",
    "babel-jest": "^29.7.0",
    "eslint": "^8.51.0",
    "jest": "^29.7.0",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  },
  "resolutions": {
    "@types/react": "^18.2.44"
  },
  "dependencies": {
    "eslint-config-expo": "^7.1.2",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-prefer-arrow": "^1.2.3"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ]
  },
  "eslintConfig": {
    "extends": [
      "expo",
      "prettier"
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "dist/"
  ]
}
